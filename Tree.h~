struct Node {
	char* name;
	char* subname;
	int show_line_num;
	int line_num;
	struct Node* brother;
	struct Node* children;
	
};

extern void create_tree_node(struct Node** node, char* name, char* subname, int is_show);
extern void connect_tree(struct Node** root, int n, ...);
extern void display_tree(struct Node* root, int blank);

