FUNCTION add :
PARAM v1
t4 := #0 * #4
t2 := v1 + t4
t5 := #1 * #4
t3 := v1 + t5
t1 := *t2 + *t3
RETURN t1
FUNCTION main :
DEC v2 8
DEC v3 8
t6 := #0
v4 := t6
t7 := #0
v5 := t7
LABEL label1 :
t8 := v4
t9 := #2
IF t8 < t9 GOTO label2
GOTO label3
LABEL label2 :
LABEL label4 :
t10 := v5
t11 := #2
IF t10 < t11 GOTO label5
GOTO label6
LABEL label5 :
t14 := v5 * #4
t13 := &v2 + t14
t15 := v4
t16 := v5
*t13 := v4 + v5
t18 := v5
t19 := #1
t17 := t18 + t19
v5 := t17
GOTO label4
LABEL label6 :
t22 := #0 * #2
t23 := t22 + v4
t24 := t23 * #4
t21 := &v3 + t24
ARG &v2
*t21 := CALL add
t26 := #0 * #2
t27 := t26 + v4
t28 := t27 * #4
t25 := &v3 + t28
t29 := *t25
WRITE t29
t31 := v4
t32 := #1
t30 := t31 + t32
v4 := t30
t33 := #0
v5 := t33
GOTO label1
LABEL label3 :
t34 := #0
RETURN t34
