.data
_prompt: .asciiz "Enter an integer:"
_ret: .asciiz "\n"
.globl main
.text
read:
	li $v0, 4
	la $a0, _prompt
	syscall
	li $v0, 5
	syscall
	jr $ra
write:
	li $v0, 1
	syscall
	li $v0, 4
	la $a0, _ret
	syscall
	move $v0, $0
	jr $ra
fact:
	move $t0, $a0
	sw $t0 -32($sp)
	lw $t0, -32($sp)
	sw $t0 -36($sp)
	li $t0, 1
	sw $t0 -40($sp)
	lw $t1, -36($sp)
	lw $t2, -40($sp)
	beq $t1, $t2, label1
	j label2
label1:
	li $t0, 9
	sw $t0 -44($sp)
	lw $t1, -44($sp)
	move $a0, $t1
	addi $sp, $sp, -48
	sw $ra, 0($sp)
	jal write
	lw $ra, 0($sp)
	addi $sp, $sp, 48
	lw $t0, -32($sp)
	sw $t0 -48($sp)
	move $v0, $v0
	jr $ra
	j label3
label2:
	lw $t0, -32($sp)
	sw $t0 -52($sp)
	li $t0, 1
	sw $t0 -56($sp)
	lw $t1, -52($sp)
	lw $t2, -56($sp)
	sub $t0, $t1, $t2
	sw $t0 -60($sp)
	lw $a0, -60($sp)
	addi $sp, $sp, -64
	sw $ra, 0($sp)
	jal fact
	move $t0, $v0
	lw $ra, 0($sp)
	addi $sp, $sp, 64
	sw $t0 -64($sp)
	lw $t0, -64($sp)
	sw $t0 -68($sp)
	lw $t0, -68($sp)
	sw $t0 -72($sp)
	lw $t1, -72($sp)
	move $a0, $t1
	addi $sp, $sp, -76
	sw $ra, 0($sp)
	jal write
	lw $ra, 0($sp)
	addi $sp, $sp, 76
	lw $t0, -32($sp)
	sw $t0 -76($sp)
	lw $t0, -32($sp)
	sw $t0 -80($sp)
	li $t0, 1
	sw $t0 -84($sp)
	lw $t1, -80($sp)
	lw $t2, -84($sp)
	sub $t0, $t1, $t2
	sw $t0 -88($sp)
	lw $a0, -88($sp)
	addi $sp, $sp, -92
	sw $ra, 0($sp)
	jal fact
	move $t0, $v0
	lw $ra, 0($sp)
	addi $sp, $sp, 92
	sw $t0 -92($sp)
	lw $t1, -76($sp)
	lw $t2, -92($sp)
	mul $t0, $t1, $t2
	sw $t0 -96($sp)
	move $t0, $v0
	jr $ra
label3:
main:
	addi $sp, $sp, -100
	sw $ra, 0($sp)
	jal read
	lw $ra, 0($sp)
	addi $sp, $sp, 100
	move $t0, $v0
	sw $t0 -100($sp)
	lw $t0, -100($sp)
	sw $t0 -104($sp)
	lw $t0, -104($sp)
	sw $t0 -108($sp)
	li $t0, 1
	sw $t0 -112($sp)
	lw $t1, -108($sp)
	lw $t2, -112($sp)
	bgt $t1, $t2, label4
	j label5
label4:
	lw $t0, -104($sp)
	sw $t0 -116($sp)
	lw $a0, -116($sp)
	addi $sp, $sp, -120
	sw $ra, 0($sp)
	jal fact
	move $t0, $v0
	lw $ra, 0($sp)
	addi $sp, $sp, 120
	sw $t0 -120($sp)
	lw $t0, -120($sp)
	sw $t0 -124($sp)
	j label6
label5:
	li $t0, 1
	sw $t0 -128($sp)
	lw $t0, -128($sp)
	sw $t0 -124($sp)
label6:
	lw $t0, -124($sp)
	sw $t0 -132($sp)
	lw $t1, -132($sp)
	move $a0, $t1
	addi $sp, $sp, -136
	sw $ra, 0($sp)
	jal write
	lw $ra, 0($sp)
	addi $sp, $sp, 136
	li $t0, 0
	sw $t0 -136($sp)
	move $v0, $v0
	jr $ra
